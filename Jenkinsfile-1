pipeline {
  agent any
  stages {
    stage('Start container') {
      steps {
        sh 'docker-compose build app'
        sh 'docker-compose up -d'
        sh 'docker-compose ps'
        sh 'cd /home/mostafa/laravel-app/laravel-crud1 && docker container exec otp-app composer install'
        sh 'cd /home/mostafa/laravel-app/laravel-crud1 && docker container exec otp-app php artisan key:generate'
      }
    }
   }
}
